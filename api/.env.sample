# Firebase Configuration - firebase json config - uglify it to make it one liner
FIREBASE_CONFIG='{"project_id": "your-project", "private_key": "..."}'

# Database Configuration
DB_CONN_STRING=host=localhost user=postgres password=password dbname=postgres port=5432 search_path=public,auth

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# API Key Configuration (Optional)
API_KEY_DEFAULT_EXPIRY_DAYS=365
API_KEY_MAX_EXPIRY_DAYS=365
RATE_LIMIT_REQUESTS_PER_MINUTE=60
SUPABASE_EMAIL_CONFIRM_REQUIRED=true
